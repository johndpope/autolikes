{
  "name": "InstagramAccount",
  "plural": "InstagramAccounts",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true,
      "required": true
    },
    "accessToken": {
      "type": "string",
      "required": true
    },
    "username": {
      "type": "string",
      "required": true,
      "index": true
    },
    "fullName": {
      "type": "string",
      "required": true
    },
    "profilePicture": {
      "type": "string",
      "required": true
    },
    "bio": {
      "type": "string"
    },
    "website": {
      "type": "string",
      "required": true
    },
    "counts": {
      "type": "object"
    },
    "likesGiven": {
      "type": "number",
      "default": 0
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "updatedAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "hidden": ["accessToken"],
  "validations": [],
  "relations": {
    "organization": {
      "type": "belongsTo",
      "model": "Organization",
      "foreignKey": "organizationId"
    },
    "settings": {
      "type": "hasOne",
      "model": "InstagramSettings",
      "foreignKey": "instagramAccountId"
    }
  },
  "acls": [],
  "methods": {},
  "mixins": {
    "ReadOnly" : {
      "likesGiven" : true
    }
  }
}
